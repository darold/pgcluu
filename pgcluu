use vars qw($VERSION $PROGRAM);

use strict qw(vars);
use warnings;

use IO::File;
use Getopt::Long qw(:config bundling no_ignore_case_always);
use Time::Local 'timegm_nocheck';
use POSIX qw(locale_h sys_wait_h);
setlocale(LC_ALL, 'C');

$VERSION = '2.0pre';
$PROGRAM = 'pgCluu';

my $OUTPUT_DIR  = '';
my $SADF_PROG   = '/usr/bin/sadf';
my $DISABLE_SAR = 0;
