}

sub build_hour_index
{
	my $outdir = shift;

	print "DEBUG: generating index to access hourly reports...\n" if ($DEBUG);

	my @done = ();
	my $fh = IO::File->new("$outdir/index.html", 'w');
	if (not defined $fh) {
		die "FATAL: can't write to $outdir/index.html, $!\n";
	}
	print $fh &build_head('Hourly Index on incremental reports', '../../..');
	print $fh qq{
<body>

<div id="navigation">

    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container mainmenu">
        <a class="navbar-brand" href="http://pgcluu.darold.net/">
        <img src="$pgcluu_logo" title="$PROGRAM"></a>
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
              <!-- li id="menu-home" class="dropdown"><a href="index.html#home">Home</a></li -->
              <!--li id="menu-about" class="dropdown"><a href="index.html#about">About</a></li -->
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>
</div>
<div class="container" id="main-container">
};

	my $date = $outdir;
	$date =~ s/^.*(\d{4})\/(\d{2})\/(\d{2}).*/$1-$2-$3/;
	print $fh qq{
	<h1>Hourly reports</h1>
	<table class="table table-striped table-hover table-condensed"><tr><td>
	<div class="panel panel-default">
	<div class="panel-heading">
	<h2>Day $date</h2>
	<div class="panel-body">
};
        print $fh "<table width='20%'><thead><tr><th colspan=4>Hours</th></tr></thead><tbody><tr>";
	foreach my $h ('00' .. '23') {
		print $fh "</tr><tr>" if ( ($h%4) == 0 && $h ne '00');
		if (-e "$outdir/$h/index.html") {
			push(@done, "$outdir/$h");
			print $fh "<td><a href=\"$h/index.html\" target=\"new\">$h:00</a></td>";
		} else {
			print $fh "<td>$h:00</td>";
		}
	}
	print $fh qq{</tr></tbody></table>
                </div>
              </div>
        </td></tr></table>
	$COPYRIGHT
</div><!--/.container-->
  </body>
</html>
};
	$fh->close;

	return @done;
}


# Returns day number of the week of a given days
sub get_day_of_week
{
	my ($year, $month, $day) = @_;

#       %w     The day of the week as a decimal, range 0 to 6, Sunday being 0.
	my $weekDay = '';
	if (!$week_start_monday) {
		# Start on sunday = 0
		$weekDay = POSIX::strftime("%w", 1,1,1,$day,--$month,$year-1900);
	} else {
		# Start on monday = 1
		$weekDay = POSIX::strftime("%u", 1,1,1,$day,--$month,$year-1900);
		$weekDay--;
	}

	return $weekDay;
}

# Return the week number of the year for a given date
sub get_week_number
{
	my ($year, $month, $day) = @_;

#       %U     The week number of the current year as a decimal number, range 00 to 53, starting with the first
#              Sunday as the first day of week 01.
#       %V     The ISO 8601 week number (see NOTES) of the current year as a decimal number, range 01 to 53,
#              where week 1 is the first week that has at least 4 days in the new year.
#       %W     The week number of the current year as a decimal number, range 00 to 53, starting with the first
#              Monday as the first day of week 01.

	# Check if the date is valid first
	my $datefmt = POSIX::strftime("%Y-%m-%d", 1, 1, 1, $day, $month - 1, $year - 1900);
	if ($datefmt ne "$year-$month-$day") {
		return -1;
	}
	my $weekNumber = '';
	if (!$week_start_monday) {
		$weekNumber = POSIX::strftime("%U", 1, 1, 1, $day, $month - 1, $year - 1900);
	} else {
		$weekNumber = POSIX::strftime("%W", 1, 1, 1, $day, $month - 1, $year - 1900);
	}
	return sprintf("%02d", $weekNumber+1);
}

sub build_daily_binary
{
	my $old_day = '';
	my $day_dir = '';
	foreach (my $dx = 0; $dx <= $#WORK_DIRS; $dx++) {

		next if ($WORK_DIRS[$dx] !~ /\d{4}\/\d{2}\/(\d{2})\/\d{2}/);
		my $curday = $1;
		my $in_dir = "$INPUT_DIR/$WORK_DIRS[$dx]";

		if ($old_day ne $curday) {
			$day_dir = "$INPUT_DIR/$WORK_DIRS[$dx]";
			$day_dir =~ s/\/\d{2}$//;
			# Save binary files for the previous day
			&dump_sys_binary($day_dir, "sys_cache.bin");
			&dump_pg_binary($day_dir);
			&dump_sar_binary($day_dir);
			&dump_pidstat_binary($day_dir);

			# Reset global variables before processing a new work dir
			&clear_stats();
			@DEVICE_LIST = ();
			@DEVICE_SPACE_LIST = ();
			@IFACE_LIST = ();

			# Switch to next day
			$old_day = $curday;
		}

		$OUTPUT_DIR = "$ORIG_OUTPUT_DIR/$WORK_DIRS[$dx]";

		# Check if we have binary file in the directory
		my @binfiles = ();
		if (-d "$in_dir") {
			opendir(IDIR, "$in_dir") || die "FATAL: can't opendir $in_dir: $!";
			@binfiles = grep { /^all_.*\.bin$/ } readdir(IDIR);
			closedir(IDIR);
		}

		# Look at system information
		&read_sysinfo($in_dir);

		if ($#binfiles > 0) {
			# Load statistics from cache files
			print "DEBUG: Loading PostgreSQL statistics from cache files $in_dir/*.bin\n" if ($DEBUG);
			&load_pg_binary($in_dir);
			if (($TIMEZONE eq '00') && $global_infos{timezone}) {
				$TIMEZONE = $global_infos{timezone};
			}
			if (($STATS_TIMEZONE eq '00') && $global_infos{stats_timezone}) {
				$STATS_TIMEZONE = $global_infos{stats_timezone};
			}
			if (!$DISABLE_SAR) {
				print "DEBUG: Loading Sar statistics from cache files $in_dir/*.bin\n" if ($DEBUG);
				&load_sar_binary($in_dir);
				&load_pidstat_binary($in_dir);
			}
		}
	}

	# Save binary files for the last day
	$day_dir = "$INPUT_DIR/$WORK_DIRS[-1]";
	$day_dir =~ s/\/\d{2}$//;
	$global_infos{'in_dir'} = $day_dir;
	# Save binary files for the last day
	&dump_sys_binary($day_dir, "sys_cache.bin");
	&dump_pg_binary($day_dir);
	&dump_sar_binary($day_dir);
	&dump_pidstat_binary($day_dir);
}

sub read_conf
{
	# We don't need a configuration file but if any check the
	# configuration directives related to pgcluu only.
	my $fh = IO::File->new($CONFIG_FILE, 'r');
	if (defined $fh)
	{
		print STDERR "DEBUG: Reading configuration file $CONFIG_FILE\n" if ($DEBUG);
		while (my $l = <$fh>)
		{
			chomp($l);
			$l =~ s/\r//gs;
			next if (!$l);

			my ($var, @vals) = split(/[\s]+/, $l);
			if ($var eq 'STATS_REPORT_RETENTION')
			{
				$vals[0] = int($vals[0]);
				if ($RETENTION != $vals[0])
				{
					$RETENTION = $vals[0];
					print STDERR "LOG: Setting retention from configuration file to $RETENTION\n";
				}
			}
			elsif ($var eq 'STATS_REPORT_CACHING')
			{
				my $tmp_outdir = '';
				for (my $i = 0; $i <= $#ARGV; $i++) {
					$tmp_outdir = $ARGV[$i+1], last if ($ARGV[$i] =~ /^(-o|--output)$/);
				}
				if (is_interactive_mode() and !$CACHE and $tmp_outdir and $vals[0]) {
					# The user might don't want to use cache mode, do nothing
					$CACHE = 0;
				} else {
					$CACHE = $vals[0];
				}
			}
			elsif ($var eq 'STATS_REPORT_OUTDIR')
			{
				$OUTPUT_DIR = $vals[0];
				print STDERR "LOG: Setting output dir from configuration file to $OUTPUT_DIR\n";
			}
		}
		close($fh);
	}
}

__DATA__

