our %sysinfo   = (); # Hash used to store system information
our %fulldata  = (); # Hash to store full content of sar file per type
our %fullpidstatdata  = (); # Hash to store full content of pidstat file per type

# Store default timestamp
my ($o_sec, $o_min, $o_hour, $o_day, $o_month, $o_year) = localtime(time);
$o_year += 1900;
$o_month++;

# Variable use to construct the date part of the sar output
# it is set with de date found in sar file kernel header.
my $sar_year  = '';
my $sar_month = '';
my $sar_day   = '';
my $pidstat_year  = '';
my $pidstat_month = '';
my $pidstat_day   = '';

# Set the default mask for file and directory creation
umask(UMASK);

# Charset used in the html output
my $charset = 'utf-8';

my $week_start_monday = 1;
my $have_incremental_dir = 0;
my $have_hourly_dir = 0;
my $pgcluu_ico = 'data:image/png;base64,';
my $pgcluu_logo = 'data:image/png;base64,';

