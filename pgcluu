my $FROM_SA_FILE = 0;
my %RELKIND = (
	'r' => 'tables',
	'i' => 'indexes',
	'S' => 'sequences',
	'v' => 'views',
	'c' => 'composite types',
	't' => 'toast tables'
);
my %HAS_TABLE_STATS  = ();
my %HAS_TABLE_STATIO = ();
my %HAS_INDEX_STATS  = ();
my %HAS_INDEX_STATIO = ();
my %HAS_FUNCTION_STATS = ();
my $TOP_STATIO       = 10;

my ($o_sec, $o_min, $o_hour, $o_day, $o_month, $o_year) = localtime(time);
$o_year += 1900;
$o_month++;

# Variable use to construct the date part of the sar output
# it is set with de date found in sar file kernel header.
my $sar_year  = '';
my $sar_month = '';
my $sar_day   = '';

# Hash used to store system information
my %sysinfo   = ();

