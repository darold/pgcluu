				foreach my $tb (sort keys %{$all_stat_hash_indexes{$db}{$sch}}) {
					next if (($#INCLUDE_TB >= 0) && !grep(/^$tb$/, @INCLUDE_TB));
					$found_stat = 1;
					foreach my $idx (sort keys %{$all_stat_hash_indexes{$db}{$sch}{$tb}}) {
